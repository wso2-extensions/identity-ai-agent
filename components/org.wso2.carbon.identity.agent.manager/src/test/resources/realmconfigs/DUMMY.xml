<UserStoreManager class="org.wso2.carbon.user.core.jdbc.UniqueIDJDBCUserStoreManager">
      <Property name="url">jdbc:mysql://localhost:3306/dummy</Property>
      <Property name="userName">dummy</Property>
      <Property name="password">dummy</Property>
      <Property name="driverName">com.mysql.jdbc.Driver</Property>
      <Property name="Disabled">false</Property>
      <Property name="ReadOnly">false</Property>
      <Property name="ReadGroups">true</Property>
